<UserControl x:Class="GraphProject.Views.TestFailListContainerView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GraphProject.Views"
             mc:Ignorable="d" 
             d:DesignHeight="884" d:DesignWidth="844"
             d:DataContext="{Binding TestFailListContainerViewModel, Source={StaticResource Locator}}">
    <Grid Height="1010" Background="Transparent">
        <Grid.Effect>
            <DropShadowEffect BlurRadius="15" Direction="-90" RenderingBias="Quality" Opacity=".5" ShadowDepth="3"/>
        </Grid.Effect>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="7*"/>
        </Grid.RowDefinitions>
        <DockPanel Grid.Row="0">
            <ContentControl Padding="10" DockPanel.Dock="Left" Content="{Binding PieContentView}"></ContentControl>
        </DockPanel>
        <ContentControl Grid.Row="1" Content="{Binding GridviewContentView}"/>

    </Grid>
</UserControl>
