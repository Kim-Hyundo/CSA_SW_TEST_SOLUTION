  )
  ELSE
  (
    SIM.UNLOAD ~~/demo/arm/simul/intc_cortexm3/nvic.dll
    SIM.LOAD   ~~/demo/arm/simul/intc_cortexm3/nvic.dll
  )
)

;enable usage, bus and memory faults
Data.Set SD:0xE000ED24 %LONG 0x00070000

;enable DIV_0_TRP, UNALIGN_TRP and USERSETMPEND
