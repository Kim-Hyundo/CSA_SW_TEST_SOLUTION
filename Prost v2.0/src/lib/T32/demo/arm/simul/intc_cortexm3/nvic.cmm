;load nvic model into TRACE32 32-bit/64-bit version.
;we use all 240-external interrupts in ports p.112 -- p.351
IF (OS.VERSION(0)>=0x20)||(OS.VERSION(0)==0x00)
(
  PRINT "Unsupported OS"
  ENDDO FALSE()
)
ELSE IF OS.VERSION(0)>=0x10
(
  IF SOFTWARE.64BIT()
  (
    SIM.UNLOAD ~~/demo/arm/simul/intc_cortexm3/nvic_pclinux64.so
    SIM.LOAD   ~~/demo/arm/simul/intc_cortexm3/nvic_pclinux64.so
  )
  ELSE
  (
    SIM.UNLOAD ~~/demo/arm/simul/intc_cortexm3/nvic_pclinux.so
    SIM.LOAD   ~~/demo/arm/simul/intc_cortexm3/nvic_pclinux.so
  )
)
ELSE
(
  IF SOFTWARE.64BIT()
  (
    SIM.UNLOAD ~~/demo/arm/simul/intc_cortexm3/nvic_x64.dll
